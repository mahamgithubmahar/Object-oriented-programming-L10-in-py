class addition:
    def __init__(self, num1, num2):
        self.num1 = num1
        self.num2 = num2

    def add(self):
        return self.num1 + self.num2

    def print_result(self):
        result = self.add()
        print(f"{self.num1}+{self.num2}={result}")

ob1=addition(3,2)
ob1.print_result()